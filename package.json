{
  "name": "mfe",
  "private": true,
  "workspaces": [
    "mfe/*",
    "packages/*"
  ],
  "scripts": {
    "sls:deploy": "serverless deploy --config serverless.yml",
    "sls:remove": "serverless remove --config serverless.yml",
    "deploy": "turbo run deploy --filter=@typer/root-config",
    "start": "turbo run start"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "dotenv": "^16.0.3",
    "lint-staged": "^12.4.1",
    "serverless": "^3.31.0",
    "serverless-s3-cleaner": "^2.0.2",
    "turbo": "^1.7.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.337.0"
  }
}
